<% layout('layout/main') -%>
<% block('title', 'Add') -%>

<h2>Create Room or Join</h2>

<img src="home.svg" class="img-fluid home__image " alt="Responsive image">

<div class="row home__buttons ">
    <form class="margin-custom col-md-4" action="/room/create" method="POST">
        <div class="form-grops">
            <button type="submit" class="btn btn-primary" >Create Room</button>
        </div>
    </form>
    <div class="col-md-4 ">
        <a type="submit" href="/room/join" class="btn btn-success" >Join Room</a>
    </div>
</div>